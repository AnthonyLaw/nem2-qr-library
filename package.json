{
  "name": "nem2-qr-library",
  "version": "0.4.0",
  "description": "nem2 library to handle QR Codes",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "canvas": "^2.6.0",
    "crypto-js": "^3.1.9-1",
    "nem2-sdk": "^0.14.3",
    "qrcode": "^1.4.0",
