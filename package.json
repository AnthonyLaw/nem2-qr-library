    "chai": "^4.1.2",
    "coveralls": "^3.0.4",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^7.0.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.5.3"
  },
  "scripts": {
